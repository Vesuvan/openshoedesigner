@BOOK{Himer2009,
  title = {Maßschuhe: Gentleman's Library},
  publisher = {Fackelträger-Verlag},
  year = {2009},
  author = {Himer, Axel},
  pages = {256},
  abstract = {Das wohl wichtigste Kleidungsstück des kultivierten Mannes ist der
	Schuh. Als Maßschuh wird er zum Synonym des guten Geschmacks, gar
	zur einzigartigen Visitenkarte seines Trägers. Dieses Buch gewährt
	detaillierte Einblicke in ein faszinierendes Handwerk, das zu Beginn
	des 21. Jahrhunderts Traditionswissen mit Hightech-Verarbeitungsmethoden
	verbindet und so auch den Ansprüchen des modernen Gentlemans genügt.
	Des Weiteren informiert der Autor über alles Wissenswerte zu Stilfragen
	und Schuhpflege.}
}

