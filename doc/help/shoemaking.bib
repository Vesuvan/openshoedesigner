@BOOK{Vass2013,
  title = {Herrenschuhe},
  publisher = {h.f.ullmann publishing},
  year = {2013},
  author = {Vass, Lászlo and Molnár, Magda},
  pages = {216},
  abstract = {Schuhe nach Maß sind Luxus für die Füße. Die Autoren dieses Buches
	begeben sich auf die Spuren einer traditionsreichen, noch heute lebendigen
	Handwerkskunst. Schritt für Schritt beschreiben sie die Entstehung
	eleganter, handgearbeiteter 'Herrenschuhe' vom ersten Maßnehmen bis
	zur professionellen Verpackung. Sie stellen klassische Modelle wie
	den Oxford oder den Slipper vor, geben Tipps zur Pflege der Schuhe
	und blicken hinter die Kulissen renommierter Schuhwerkstätten in
	ganz Europa. Exkurse zur Geschichte der Schuhmode und des Schusterhandwerks
	stehen neben exzellenten Farbaufnahmen zu jeder Phase in der Entstehung
	der feinen Lederkunstwerke. Ein Buch wie maßgeschneidert für alle
	traditions- und modebewussten Schuhliebhaber.}
}

